<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ly.customui" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WaterFallActivity"
            android:label="@string/title_activity_water_fall" >
        </activity>
        <activity
            android:name=".RingActivity"
            android:label="@string/title_activity_ring" >
        </activity>
        <activity
            android:name=".SeniorRingActivity"
            android:label="@string/title_activity_senior_ring" >
        </activity>
    </application>

</manifest>
